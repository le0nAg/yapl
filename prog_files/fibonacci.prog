fn fibonacci(int n) int {
    if (n <= 1) {
        return n;
    }
    return fibonacci(n - 1) + fibonacci(n - 2);
}

fn main() void {
    print("Fibonacci Calculator");
    print("Enter a number:");
    
    int n = read();
    
    print("Calculating fibonacci(", n, ")...");
    
    int result = fibonacci(n);
    
    print("Result:", result);
}